<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    tools:context=".b1YearTwoActivity">



    <TextView
        android:id="@+id/textView"
        android:layout_width="200dp"
        android:layout_height="54dp"
        android:layout_centerHorizontal="true"
        android:layout_above="@id/imageButtonVolume"
        android:layout_marginBottom="10dp"
        android:text="@string/b1QuestionYeartwo"
        android:textAlignment="center"
        android:textColor="@color/colorText"
        android:textSize="22dp" />

    <ImageButton
        android:id="@+id/imageButtonVolume"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@id/imageView2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:background="@color/colorGrid"
        app:srcCompat="@drawable/ic_volume_up_black_24dp" />


    <ImageButton
        android:id="@+id/imageButtonVolume2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@id/imageButtonVolume"
        android:layout_toLeftOf="@id/imageView2"
        android:layout_marginTop="35dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrid"
        app:srcCompat="@drawable/ic_volume_up_black_24dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_centerInParent="true"
        android:layout_above="@id/textView2"
        android:layout_marginBottom="1dp"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/imageView2"
        android:layout_marginBottom="0dp"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/imageButtonVolume4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@id/imageView2"
        android:layout_toLeftOf="@id/imageView3"
        android:layout_marginTop="35dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrid"
        app:srcCompat="@drawable/ic_volume_up_black_24dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:layout_below="@id/imageView3"
        android:layout_centerHorizontal="true"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/imageButtonVolume5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@id/imageView3"
        android:layout_toLeftOf="@id/imageView4"
        android:layout_marginTop="35dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrid"
        app:srcCompat="@drawable/ic_volume_up_black_24dp" />





</RelativeLayout>